ot>			</game>			<game name="gtmr" sourcefile="kaneko16.c">					<description>1000 Miglia: Great 1000 Miles Rally (94/07/18)</description>					<year>01/01/1994</year>					<manufacturer>Kaneko</manufacturer>					<category>Driving / Race</category>					<nplayers>2P alt</nplayers>					<verAdded>.036b16</verAdded>					<controls_info>numPlayers=&quot;2&quot; alternating=&quot;1&quot; mirrored=&quot;1&quot; usesService=&quot;0&quot; tilt=&quot;1&quot; cocktail=&quot;1&quot;</controls_info>					<player numButtons="1" number="1">							<controls>									<control name="270 Steering Wheel">											<constant name="paddle"/>									</control>									<control name="Pedal (Microswitch)">											<constant name="button"/>											<button name="P1_BUTTON1"/>											</control>							</controls>									<labels>									<label name="P1_BUTTON1" value="Accelerate"/>									<label name="P1_PADDLE" value="Left"/>									<label name="P1_PADDLE_EXT" value="Right"/>							</labels>							</player>					<rom crc="b9cbfbee" name="mm-100-401-e0.bin" sha1="051d48a68477ef9c29bd5cc0bb7955d513a0ab94" size="1048576"/>					<rom crc="c0ab3efc" name="mm-200-402-s0.bin" sha1="e6cd15480977b036234d91e6f3a6e21b7f0a3c3e" size="2097152"/>					<rom crc="cf6b23dc" name="mm-201-403-s1.bin" sha1="ccfd0b17507e091e55c169361cd6a6b19641b717" size="2097152"/>					<rom crc="8f27f5d3" name="mm-202-404-s2.bin" sha1="219a86446ce2556682009d8aff837480f040a01e" size="2097152"/>					<rom crc="e9747c8c" name="mm-203-405-s3.bin" sha1="2507102ec34755c6f110eadb3444e6d3a3474051" size="524288"/>					<rom crc="b15f6b7f" name="mm-300-406-a0.bin" sha1="5e84919d788add53fc87f4d85f437df413b1dbc5" size="2097152"/>					<rom crc="3d7cb329" name="mmd0x2.u124.bin" sha1="053106acde642a414fde0b01105fe6762b6a10f6" size="131072"/>					<rom crc="bd22b7d2" name="mms0x2.u29.bin" sha1="ef82d00d72439590c71aed33ecfabc6ee71a6ff9" size="131072"/>					<rom crc="b42b426f" name="mms1x2.u30.bin" sha1="6aee5759